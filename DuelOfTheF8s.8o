: maybe_delay
	do_delay
;

: do_delay
	loop
	  vf := delay
	  if vf != 0 then
	again

:next delay_target
	vf := delay_value_slow
	delay := vf
;

: intro_wait
	v2 := 116
	v3 := 56
	i := waiting_sprite
	sprite v2 v3 8
	v0 := 6
	# Debounce:
	loop
		if v0 -key then
	again
	loop
		if v0 key then
	again
	sprite v2 v3 8
;
	
: draw_theed_power_wall	
	# Uses v0-v1
	# Draw the sides:
	i := long handy_all_fs
	plane 1
	v0 := 0
	v1 := 0
	loop
		loop
			sprite v0 v1 0
			v1 += 16
			if v1 != 64 then
		again
		v1 := 0
		v0 += 16
		if v0 == 48 then v0 := 80

		if v0 != 128 then
	again

	# Draw the floor:
	v0 := 48
	v1 := 56
	loop
		sprite v0 v1 8
		v0 += 8
		if v0 != 80 then
	again


	# Draw the part that's above the intro sprite
	i := long handy_all_fs
	plane 2
	v0 := 48
	loop
		v1 := 0
		sprite v0 v1 12
		v1 := 12
		sprite v0 v1 12
		v0 += 8
		if v0 != 80 then
	again
	plane 1
;



# DrawText 0xFF means end. 0xFE means carriage return
: drawtext
	# Arguements: v1 v2 v3 - start x, start y, carriage return x
	# Uses: v0 v1 v2 v3 vf

	# v1 v2 will be start x/y
	# v0 will be used too, for loading purposes
	
	# Must have initialised vf := 0
	# and run this in a loop
	# setting i:= the text, then calling this, break their loop if vf == -1

	# Step to character:
	i += vf
	# load char into v0
	load v0
	# Test for control chars:
	if v0 == -1 begin
		# we've reached the end
		vf := -1
		return
		end
	# Step on character pointer while we're here
	vf += 1
	# Detect carriage return:
	if v0 == -2 begin
		# Set x to cr point
		v1 := v3
		# step down by 6 because 5 wide
		v2 += 6

		else

		# Draw character
		i := fivewide_extra
		i += v0
		# Make sure we retain vf:
		v0 := vf
		sprite v1 v2 5
		# kinda lame but what can you do
		vf := v0
		# Step forward to the right
		v1 += 6

		end
;

: draw_intro_pose
	# uses v0-v3
	i := long intro_frames
	plane 3

	v0 := 48
	v1 := 24 #64 - 8 - 32
	v2 := 16


	v3 := 128
	# Step forward as ufficient number of frames
	if ve != 0 begin
		vf := ve
		loop
			i += v3
			i += v3
			vf += -1
			if vf != 0 then
		again
	end
	v3 := 64
	
	loop
		v1 := 24
		sprite v0 v1 0
		v1 += v2
		i += v3
		sprite v0 v1 0
		i += v3
		v0 += v2
		if v0 != 80 then
	again
	plane 1
;


