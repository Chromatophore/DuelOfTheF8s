# " abcdefghijklmnopqrstuvwxyz!012345679-.,'"

:const delay_value_slow 0x0a
:const delay_value_fast 0x02

# Fine I'll try using a macro this octojam:
:macro auto_text txt X Y {
	vf := 0
	v1 := X
	v2 := Y
	#v3 := CR
	loop
		i := txt
		drawtext
		if vf != 0xff then
	again
}



