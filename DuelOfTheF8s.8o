	intro_wait

	auto_text intro_2_4 16 47

#	auto_text intro_1 1 16 1
	#auto_text intro_2 1 22 1

	intro_wait
	clear

	intro_3_text

	intro_wait
	clear
	draw_theed_power_wall

	ve := 0
	draw_intro_pose
	intro_wait
	plane 3
	clear
	plane 1


	i := maybe_delay
	# set it to return
	v0 := 00
	v1 := 0xEE
