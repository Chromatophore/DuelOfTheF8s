	# redraw that text:
	intro_3_text
	# and now the new stuff:
	auto_text intro_4_1 48 32
	intro_wait
	auto_text intro_4_2 37 47

	intro_wait
	clear

	i := maybe_delay
	# set it back to run the subroute
	:unpack 0x2 do_delay
	save v1

	intro_5_text

	intro_wait
	# Stands
	clear
	draw_theed_power_wall
	ve := 0

	# do a long delay to begin with
	vf := 0x20
	delay := vf

	loop
		draw_intro_pose
		do_delay
		draw_intro_pose
		ve += 1
		if ve != 7 then
	again
	# leave standing:
	draw_intro_pose
	plane 1
	intro_wait
	plane 3
	clear
	plane 1

	i := maybe_delay
	# set it to return
	v0 := 00
	v1 := 0xEE
	save v1

	intro_5_text
	do_delay
	auto_text intro_5_4 55 29

	intro_wait

	do_delay
	auto_text intro_6_1 13 41
	do_delay
	auto_text intro_6_2 13 47

	intro_wait
	clear

	do_delay
	auto_text intro_7_1 37 23
	do_delay
	auto_text intro_7_2 1 29
	do_delay
	auto_text intro_7_3 32 38

	intro_wait
	clear

	draw_theed_power_wall
	# do a long delay to begin with

	i := delay_target
	v0 := delay_value_fast
	save v0

	vf := 0x20
	delay := vf
	ve := 7

	# ignite
	loop
		draw_intro_pose
		do_delay
		draw_intro_pose
		ve += 1
		if ve != 11 then
	again
	draw_intro_pose
	plane 1
	intro_wait
	plane 3
	clear
;

